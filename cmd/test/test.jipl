function isPrime(n) {
    if (n < 2) {
        return false;
    } else {
        for (def i = 2; i < n; i++) {
            if (n % i == 0) {
                return false;
            }
        }
        return true;
    }
};

out("here test 7 ");

out(isPrime(7));

out("here test 2");

